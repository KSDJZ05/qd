TextButton.MouseButton1Click:Connect(function()
    local inputKey = KeyTextbox.Text
    
    -- 过期卡密检查
    local expiredKeys = {
        "ilove78", "91vip", "群号", "xiaoai", "dajibaccb", "iamgay"
    }
    
    for _, expiredKey in ipairs(expiredKeys) do
        if inputKey == expiredKey then
            TextButton.Text = "该卡密已过期，加群获得最新卡密😘!"
            task.wait(4)
            TextButton.Text = "确定"
            return
        end
    end
    
    -- 有效卡密检查
    if inputKey == key then
        TextButton.Text = "正确!"
        task.wait(2)
        loadstring(game:HttpGet("https://raw.githubusercontent.com/KSDJZ05/qd/refs/heads/main/58"))()
        ScreenGui:Destroy()
    else
        TextButton.Text = "错误的卡密!"
        task.wait(2)
        TextButton.Text = "确定"
    end
end)

