    -- 3.7 æœåŠ¡å™¨çŠ¶æ€æ˜¾ç¤ºï¼ˆæ—¶é—´/FPSï¼‰
    BrainRedTab:AddButton({
        Name = "æ˜¾ç¤ºæœåŠ¡å™¨çŠ¶æ€ï¼ˆæ—¶é—´+FPSï¼‰",
        Callback = function()
            -- åˆ›å»ºçŠ¶æ€UI
            local StatusGui = Instance.new("ScreenGui")
            StatusGui.Name = "ServerStatus_GUI"
            StatusGui.Parent = game.CoreGui
            StatusGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
            -- FPS/æ—¶é—´æ˜¾ç¤ºæ ‡ç­¾
            local StatusLabel = Instance.new("TextLabel")
            StatusLabel.Parent = StatusGui
            StatusLabel.BackgroundColor3 = Color3.new(0, 0, 0)
            StatusLabel.BackgroundTransparency = 0.5
            StatusLabel.Position = UDim2.new(0.8, 0, 0.01, 0)
            StatusLabel.Size = UDim2.new(0, 180, 0, 40)
            StatusLabel.Font = Enum.Font.GothamSemibold
            StatusLabel.TextColor3 = Color3.new(1, 1, 1)
            StatusLabel.TextScaled = true
            StatusLabel.TextWrapped = true
            -- è®¡ç®—FPSé€»è¾‘
            local LastIteration, Start = tick(), tick()
            local FrameTable = {}
            game:GetService("RunService").Heartbeat:Connect(function()
                local CurrentTime = tick()
                table.insert(FrameTable, CurrentTime)
                -- åªä¿ç•™1ç§’å†…çš„å¸§æ•°æ®
                while FrameTable[1] < CurrentTime - 1 do
                    table.remove(FrameTable, 1)
                end
                -- æ›´æ–°æ˜¾ç¤ºæ–‡æœ¬
                local FPS = #FrameTable
                local BeijingTime = os.date("%H:%M:%S", os.time() + 28800) -- è½¬æ¢ä¸ºåŒ—äº¬æ—¶é—´ï¼ˆ+8å°æ—¶ï¼‰
                StatusLabel.Text = string.format("åŒ—äº¬æ—¶é—´ï¼š%s\nFPSï¼š%d", BeijingTime, FPS)
            end)
            OrionLib:MakeNotification({
                Title = "çŠ¶æ€æ˜¾ç¤º",
                Text = "å·²åœ¨å³ä¸Šè§’æ˜¾ç¤ºæ—¶é—´ä¸FPS",
                Duration = 2
            })
        end
    })
