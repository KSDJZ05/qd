local function StartMainScript()
    -- åˆå§‹åŒ–ä¸»çª—å£
    local MainWindow = OrionLib:MakeWindow({
        Name = "æ²³åŒ—å”å¿æ­»é“è½¨/å·èµ°è„‘çº¢è¾…åŠ©",
        HidePremium = false,
        SaveConfig = true,
        IntroText = "åŠŸèƒ½é€‚é…æŒ‡å®šæœåŠ¡å™¨ï¼Œè¯·å‹¿æ»¥ç”¨",
        ConfigFolder = "RailBrain_Aux"
    })

    -- æ–°å¢ï¼šåœ¨ä¸»åŠŸèƒ½æ‚¬æµ®çª—å³ä¾§æ·»åŠ è®¤è¯å›¾æ ‡ï¼ˆOrionLibçª—å£éœ€å®šä½åˆ°å®¹å™¨èŠ‚ç‚¹ï¼‰
    -- å»¶è¿Ÿ0.1ç§’ç¡®ä¿çª—å£UIåŠ è½½å®Œæˆ
    task.wait(0.1)
    local MainWindowContainer = MainWindow.Window -- è·å–OrionLibçª—å£çš„æ ¹å®¹å™¨
    if MainWindowContainer then
        local MainCertIcon = CreateCertIcon(MainWindowContainer)
        -- è°ƒæ•´ä¸»çª—å£å›¾æ ‡ä½ç½®ï¼ˆé€‚é…OrionLibé»˜è®¤UIå°ºå¯¸ï¼‰
        MainCertIcon.Position = UDim2.new(1, -40, 0.5, -15)
    end

    -- ====================== é€šç”¨åŠŸèƒ½Tab ======================
    local GeneralTab = MainWindow:MakeTab({
        Name = "é€šç”¨åŠŸèƒ½",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })
 -- 3.1 ç§»é€Ÿè°ƒèŠ‚ï¼ˆæ”¯æŒè‡ªå®šä¹‰æ•°å€¼ï¼‰
    GeneralTab:AddTextbox({
        Name = "ç§»åŠ¨é€Ÿåº¦è®¾ç½®",
        Default = "16", -- é»˜è®¤Robloxç§»é€Ÿ
        TextDisappear = true,
        Callback = function(Value)
            local Speed = tonumber(Value)
            if not Speed then return end
            local LocalPlayer = game.Players.LocalPlayer
            LocalPlayer.CharacterAdded:Connect(function(Char)
                local Humanoid = Char:WaitForChild("Humanoid")
                Humanoid.WalkSpeed = Speed
            end)
            -- å®æ—¶æ›´æ–°å½“å‰è§’è‰²ç§»é€Ÿ
            if LocalPlayer.Character then
                local Humanoid = LocalPlayer.Character:FindFirstChild("Humanoid")
                if Humanoid then Humanoid.WalkSpeed = Speed end
            end
            OrionLib:MakeNotification({
                Title = "ç§»é€Ÿè®¾ç½®æˆåŠŸ",
                Text = "å½“å‰ç§»é€Ÿï¼š" .. Speed,
                Duration = 2
            })
        end
    })

    -- 3.2 é£è¡ŒåŠŸèƒ½ï¼ˆV3ç‰ˆæœ¬ï¼Œé€‚é…æœåŠ¡å™¨ç‰©ç†ï¼‰
    local FlyEnabled = false
    local FlyForce = 50
    GeneralTab:AddButton({
        Name = "é£è¡ŒV3ï¼ˆå¼€å…³ï¼‰",
        Callback = function()
            FlyEnabled = not FlyEnabled
            local LocalPlayer = game.Players.LocalPlayer
            local Character = LocalPlayer.Character
            if not Character then return end
            local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")
            local Humanoid = Character:WaitForChild("Humanoid")
            -- é£è¡Œé€»è¾‘å¾ªç¯
            game:GetService("RunService").Heartbeat:Connect(function()
                if not FlyEnabled or not Character:IsDescendantOf(game.Workspace) then return end
                Humanoid.PlatformStand = true -- é˜²æ­¢è½åœ°åˆ¤å®š
                local Camera = game.Workspace.CurrentCamera
                local Direction = Vector3.new()
                -- é”®ç›˜æ§åˆ¶æ–¹å‘
                if
                game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.W) then
                    Direction = Direction + Camera.CFrame.LookVector
                end
                if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.S) then
                    Direction = Direction - Camera.CFrame.LookVector
                end
                if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.A) then
                    Direction = Direction - Camera.CFrame.RightVector
                end
                if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.D) then
                    Direction = Direction + Camera.CFrame.RightVector
                end
                if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.Space) then
                    Direction = Direction + Vector3.new(0, 1, 0)
                end
                if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.LeftControl) then
                    Direction = Direction - Vector3.new(0, 1, 0)
                end
                -- åº”ç”¨é£è¡ŒåŠ›
                if Direction.Magnitude > 0 then
                    HumanoidRootPart.Velocity = Direction.Unit * FlyForce
                end
            end)
            -- æç¤ºé£è¡ŒçŠ¶æ€
            OrionLib:MakeNotification({
                Title = "é£è¡ŒåŠŸèƒ½",
                Text = FlyEnabled and "å·²å¼€å¯ï¼ˆWASDæ§åˆ¶æ–¹å‘ï¼Œç©ºæ ¼ä¸Šå‡ï¼ŒCtrlä¸‹é™ï¼‰" or "å·²å…³é—­",
                Duration = 3
            })
        end
    })

    -- 3.3 ç©¿å¢™åŠŸèƒ½ï¼ˆç¦ç”¨ç¢°æ’ï¼‰
    local NoclipEnabled = false
    GeneralTab:AddButton({
        Name = "ç©¿å¢™ï¼ˆå¼€å…³ï¼‰",
        Callback = function()
            NoclipEnabled = not NoclipEnabled
            local LocalPlayer = game.Players.LocalPlayer
            LocalPlayer.CharacterAdded:Connect(function(Char)
                for _, Part in ipairs(Char:GetChildren()) do
                    if Part:IsA("BasePart") then
                        Part.CanCollide = not NoclipEnabled
                    end
                end
                -- ç›‘å¬æ–°ç”Ÿæˆçš„éƒ¨ä»¶ï¼ˆå¦‚æ­¦å™¨ã€è£…å¤‡ï¼‰
                Char.ChildAdded:Connect(function(Part)
                    if Part:IsA("BasePart") then
                        Part.CanCollide = not NoclipEnabled
                    end
                end)
            end)
            -- å®æ—¶æ›´æ–°å½“å‰è§’è‰²ç¢°æ’
            if LocalPlayer.Character then
                for _, Part in ipairs(LocalPlayer.Character:GetChildren()) do
                    if Part:IsA("BasePart") then
                        Part.CanCollide = not NoclipEnabled
                    end
                end
            end
            OrionLib:MakeNotification({
                Title = "ç©¿å¢™åŠŸèƒ½",
                Text = NoclipEnabled and "å·²å¼€å¯ï¼ˆæ³¨æ„ï¼šéƒ¨åˆ†æœåŠ¡å™¨å¯èƒ½æ£€æµ‹ï¼‰" or "å·²å…³é—­",
                Duration = 2
            })
        end
    })

    -- ====================== æ­»é“è½¨ä¸“ç”¨Tab ======================
    local DeathRailTab = MainWindow:MakeTab({
        Name = "æ­»é“è½¨ä¸“ç”¨",
        Icon = "rbxassetid://7734068321",
        PremiumOnly = false
    })

    -- 3.4 ä¹Œé¾Ÿåˆ·å€ºåˆ¸ï¼ˆé€‚é…æ­»é“è½¨æœåŠ¡å™¨ï¼‰
