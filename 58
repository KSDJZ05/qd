        Name = "ä¹Œé¾Ÿåˆ·å€ºåˆ¸ï¼ˆè‡ªåŠ¨æ‰§è¡Œï¼‰",
        Callback = function()
            local success, err = pcall(function()
                getgenv().AutoExecute = true
                loadstring(game:HttpGet("https://raw.githubusercontent.com/hungquan99/HungHUB/main/loader.lua"))()
            end)
            if success then
                OrionLib:MakeNotification({
                    Title = "åˆ·å€ºåˆ¸åŠŸèƒ½",
                    Text = "å·²å¯åŠ¨ï¼Œè¯·æ³¨æ„æœåŠ¡å™¨è§„åˆ™",
                    Duration = 2
                })
            else
                OrionLib:MakeNotification({
                    Title = "åŠŸèƒ½é”™è¯¯",
                    Text = "åŠ è½½å¤±è´¥ï¼š" .. err,
                    Duration = 3
                })
            end
        end
    })

    -- 3.5 è¿‘æˆ˜æ­¦å™¨æ”»é€Ÿæå‡ï¼ˆæ­»é“è½¨ä¸“ç”¨ï¼‰
    DeathRailTab:AddButton({
        Name = "è¿‘æˆ˜æ­¦å™¨æ”»é€Ÿå¢å¼º",
        Callback = function()
            local success, err = pcall(function()
                loadstring(game:HttpGet("https://raw.githubusercontent.com/HeadHarse/DeadRails/refs/heads/main/V4SWING"))()
