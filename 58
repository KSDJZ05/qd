-- 在 StartMainScript() 函数前添加 CreateCertIcon 函数
local function CreateCertIcon(parent)
    local CertIcon = Instance.new("ImageLabel")
    CertIcon.Name = "CertificationIcon"
    CertIcon.Parent = parent
    CertIcon.BackgroundTransparency = 1
    CertIcon.Size = UDim2.new(0, 30, 0, 30)
    CertIcon.Image = "rbxassetid://7734068321" -- 使用现有图标
    return CertIcon
end

